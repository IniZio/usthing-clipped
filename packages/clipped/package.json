{
  "name": "clipped",
  "description": "Reduce pain of configuration to once",
  "version": "1.11.6",
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clippedjs/clipped.git"
  },
  "author": "IniZio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clippedjs/clipped/issues"
  },
  "main": "dist/index.js",
  "bin": {
    "clipped": "clipped.js"
  },
  "scripts": {
    "flow:deps": "flow-typed install",
    "//postinstall": "npm run flow:deps",
    "dev": "clipped watch",
    "build": "clipped build",
    "pretest": "cross-env NODE_ENV=test clipped test",
    "test": "nyc --reporter=html --reporter=text mocha test-dist",
    "coverage": "nyc report --reporter=text-lcov | ../../node_modules/.bin/coveralls",
    "semantic-release": "semantic-release"
  },
  "standard": {
    "globals": [
      "clippedInstance",
      "clippedConfig",
      "stream$Readable",
      "beforeEach",
      "describe",
      "it"
    ],
    "ignore": [
      "**/flow-typed/",
      "**/dist/"
    ],
    "parser": "babel-eslint",
    "extends": [
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "define-flow-type": 1,
      "use-flow-type": 1,
      "flowtype/define-flow-type": 2
    }
  },
  "homepage": "https://github.com/clippedjs/clipped#readme",
  "dependencies": {
    "artzlogger": "^0.0.12",
    "cosmiconfig": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "dargs": "^5.1.0",
    "dockerode": "^2.5.3",
    "extends-classes": "^1.0.5",
    "fs-extra": "^5.0.0",
    "generator-clipped-base": "^1.12.0",
    "git-url-parse": "^7.0.1",
    "jointed": "^1.7.10",
    "lodash": "^4.17.4",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "simple-git": "^1.84.0",
    "source-map-support": "^0.5.0",
    "tar-fs": "^1.16.0",
    "update-notifier": "^2.3.0",
    "webpack-merge": "^4.1.1",
    "yarn-install": "^0.5.1",
    "yeoman-environment": "^2.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "backpack-core": "^0.4.3",
    "codacy-coverage": "^2.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.62.0",
    "flow-typed": "^2.2.3",
    "glob": "^7.1.2",
    "imports-loader": "^0.7.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^11.0.2",
    "shebang-loader": "^0.0.1",
    "standard": "^10.0.3",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
