{
  "name": "clipped",
  "description": "Reduce pain of configuration to once",
  "version": "1.17.0",
  "engines": {
    "node": ">=8"
  },
  "author": "IniZio",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "clipped": "bin/clipped"
  },
  "homepage": "https://github.com/clippedjs/clipped#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/clippedjs/clipped.git"
  },
  "bugs": {
    "url": "https://github.com/clippedjs/clipped/issues"
  },
  "scripts": {
    "type:deps": "typesync ./package.json",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "tsc && ava",
    "test:watch": "tsc --watch & ava --watch",
    "lint": "xo",
    "semantic-release": "semantic-release"
  },
  "xo": {
    "extends": "xo-typescript",
    "semicolon": false,
    "space": 2,
    "extensions": [
      "ts"
    ],
    "ignores": [
      "test"
    ],
    "globals": [
      "describe",
      "beforeEach",
      "it"
    ],
    "rules": {
      "capitalized-comments": 0
    }
  },
  "ava": {
    "files": [
			"dist/test/**/*.test.js"
		],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "artzlogger": "^0.0.12",
    "cosmiconfig": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "dargs": "^5.1.0",
    "dockerode": "^2.5.3",
    "extends-classes": "^1.0.5",
    "fs-extra": "^5.0.0",
    "generator-clipped-base": "^1.12.0",
    "git-url-parse": "^7.0.1",
    "jointed": "^1.7.11",
    "lodash": "^4.17.4",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "simple-git": "^1.84.0",
    "source-map-support": "^0.5.0",
    "tar-fs": "^1.16.0",
    "update-notifier": "^2.3.0",
    "webpack-merge": "^4.1.1",
    "yarn-install": "^0.5.1",
    "yeoman-environment": "^2.0.5"
  },
  "devDependencies": {
    "@types/cosmiconfig": "^5.0.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/dargs": "^5.1.0",
    "@types/dockerode": "^2.5.5",
    "@types/eslint": "^4.16.4",
    "@types/fs-extra": "^5.0.4",
    "@types/git-url-parse": "^9.0.0",
    "@types/glob": "^7.1.1",
    "@types/istanbul": "^0.4.30",
    "@types/lodash": "^4.14.117",
    "@types/mem-fs": "^1.1.1",
    "@types/mem-fs-editor": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/ncp": "^2.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/source-map-support": "^0.4.1",
    "@types/tar-fs": "^1.16.1",
    "@types/update-notifier": "^2.2.0",
    "@types/webpack": "^4.4.17",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "ava": "^1.0.0-rc.1",
    "cross-env": "^5.1.3",
    "eslint-config-xo-typescript": "^0.2.0",
    "eslint-plugin-typescript": "^0.12.0",
    "glob": "^7.1.2",
    "pre-commit": "^1.2.2",
    "semantic-release": "^11.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0",
    "typesync": "^0.4.0",
    "xo": "^0.23.0"
  }
}
